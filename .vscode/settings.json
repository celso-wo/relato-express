{
  "files.exclude": {
    ".editorconfig": true,
    ".eslintrc": false,
    ".gitignore": true,
    ".jshintrc": true,
    ".jsbeautifyrc": true,
    "client/node_modules/": true,
    "server/node_modules/": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.map": {
      "when": "$(basename).map"
    },
    "**/.git": true,
    "**/.DS_Store": true
  },
  "vsicons.presets.angular": false
}
